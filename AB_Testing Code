        "# If purchased before...\n",
        "purchased = df[df['made_purchase_before'] == 1]\n",
        "purchased_spend_rate = len(purchased[purchased['spend_usd_next_14_days'] != 0]) / (len(purchased) + 0.0)\n",
        "\n",
        "# print reuslts\n",
