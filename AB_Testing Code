        "# draw bar plot\n",
        "res_fop_df = pd.DataFrame(columns=['fop', 'multiple_fop', 'single_fop'])\n",
        "res_fop_df['fop'] = target_features\n",
        "res_fop_df['multiple_fop'] = multiple_fop\n",
        "res_fop_df['single_fop'] = single_fop\n",
        "print ('FOP conversion rate:')\n",
        "print (res_fop_df)\n",
        "\n",
